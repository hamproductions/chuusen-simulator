{
  "name": "chuusen-simulator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "tsc && bunx --bun vite build",
    "ci:build": "NODE_ENV=production bunx --bun vite build",
    "type-check": "tsc",
    "check": "bun lint && bun type-check",
    "ci:check": "bun lint --format json --output-file lint_report.json && bun type-check",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "fix": "bun lint:fix",
    "preview": "bunx --bun vite preview",
    "prepare": "panda codegen",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage.enabled",
    "test:preview": "vitest-preview && open https://localhost:5006",
    "test:dev": "bunx concurrently \"TEST_PREVIEW=true bun test:ui\" \"bun test:preview\"",
    "ci:test": "vitest --coverage.enabled",
    "analyze": "bunx vite-bundle-visualizer",
    "codegen:components": "bunx @park-ui/cli components add --all",
    "release": "release-it --ci",
    "release:patch": "release-it patch --ci",
    "release:minor": "release-it minor --ci",
    "release:major": "release-it major --ci",
    "update-version": "bun scripts/update-version-release-it.ts",
    "install-hooks": "chmod +x .git/hooks/pre-commit && chmod +x scripts/check-version.ts",
    "check-version": "bun scripts/check-version.ts"
  },
  "dependencies": {
    "@ark-ui/react": "^5.18.3",
    "@pandacss/dev": "^1.0.0",
    "@park-ui/panda-preset": "^0.43.1",
    "@sentry/react": "^10.8.0",
    "@stdlib/stats-base-dists-lognormal": "^0.2.2",
    "i18next": "^25.3.2",
    "mathjs": "^14.6.0",
    "path-browserify": "^1.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.6.1",
    "react-icons": "^5.5.0",
    "recharts": "^3.1.2",
    "vike": "^0.4.236",
    "vike-react": "^0.6.5",
    "vite-plugin-cjs-interop": "^2.2.0"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.10.3",
    "@eslint/compat": "^1.3.1",
    "@pandabox/prettier-plugin": "^0.1.3",
    "@pandacss/eslint-plugin": "^0.2.12",
    "@pandacss/studio": "^1.0.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "bun-types": "^1.2.19",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.1.4",
    "file-type": "^21.0.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "release-it": "^19.0.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.0.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-preview": "^0.0.1"
  }
}
